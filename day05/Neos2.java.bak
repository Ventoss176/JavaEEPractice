class Neos2 {
	public static void main(String[] args) {
		int[] studentScoresTroop = new int[]{4,3,2,1};
		int max1 = 0;
		for(int i = 0;i < 5;i++) {
			for(int j = 0;j < 5;j++) {
				if(studentScoresTroop[j] >= studentScoresTroop[i]) {
					if(studentScoresTroop[j] >= max1) {
						max1 = studentScoresTroop[j];
					}
				}
			}
		}
		System.out.println("最高分为：" + max1);
	}
		
}
